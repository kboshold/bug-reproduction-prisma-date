generator client {
  provider   = "prisma-client"
  output     = "./prisma/client"
  engineType = "library"
}

datasource db {
  provider  = "postgres"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

model TestData {
  id   String    @id @default(uuid())
  date DateTime?
}
